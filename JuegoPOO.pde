int pantalla;
boolean player1 = false;
boolean player2 = false;
int turno;
int vida1;
int vida2;
int select;
int p2select;

Eigthbff p1a;
Eigthbff p2a;
Eigthbff p3a;
Eigthbff p4a;
Eigthbff p5a;

Eigthbff p1b;
Eigthbff p2b;
Eigthbff p3b;
Eigthbff p4b;
Eigthbff p5b;


Eigthbff j1;
Eigthbff j2;


void setup() {
  size(1280, 700);
  p1a = new Eigthbff();
  p2a = new Eigthbff();
  p3a = new Eigthbff();
  p4a = new Eigthbff();
  p5a = new Eigthbff();

  p1b = new Eigthbff();
  p2b = new Eigthbff();
  p3b = new Eigthbff();
  p4b = new Eigthbff();
  p5b = new Eigthbff();
}


void draw() {
  switch(pantalla) {
  case 0:
    inicio();
    break;
  case 1:
    seleccion();
    break;
  case 2:
    juego();
    break;
  case 3:
    fin();
    break;
  } 
  println(pantalla);
}


void inicio() {
  pushMatrix();
  for (int i=0; i<=300; i+=2)
  {
    if (i<=300) {
      fill((10),random(45, 50),(50));
      rect(i, i, 1280-(i*2), 700-(i*2));
    }
  }
  popMatrix();

  pushMatrix();
  textSize(42);
  fill(random(0, 255), random(0, 255), random(0, 0));
  text("Insert Coin (ESPACIO)", 410, 360);
  popMatrix();
  keyPressed();
  if (key == ' ') {
    pantalla = 1;
  }
}


void seleccion() {
  background(0);


  p1a.displayer();
  p2a.displayer();
  p3a.displayer();
  p4a.displayer();
  p5a.displayer();
  
  if (player1 && player2) {
    pantalla = 2;
    turno += 1;
  }

 
  if (keyPressed) {
    switch(key) {
    case 'q':
      j1 = p1a;
      vida1=j1.vida;
      player1 = true;
      break;
      
    case 'w':
      j1 = p2a;
      vida1=j1.vida;
      player1 = true;
      break;
      
    case 'e':
      j1 = p3a;
      vida1=j1.vida;
      player1 = true;
      break;
      
    case 'r':
      j1 = p4a;
      vida1=j1.vida;
      player1 = true;
      break;
      
    case 't':
      j1 = p5a;
      vida1=j1.vida;
      player1 = true;
      break;
      
    case 'a':
      j2 = p1b;
      vida2=j2.vida;
      player2 = true;
      break;
      
    case 's':
      j2 = p2b;
      vida2=j2.vida;
      player2 = true;
      break;
    case 'd':
      j2 = p3b;
      vida2=j2.vida;
      player2 = true;
      break;
      
    case 'f':
      j2 = p4b;
      vida2=j2.vida;
      player2 = true;
      break; 
      
    case 'g':
      j2 = p5b;
      vida2=j2.vida;
      player2 = true;
      break;
    }
  }

  pushMatrix();
  textSize(30);
  fill(random(200, 255));
  stroke(0);
  String s = "Player 1 choose     {Q}  {W}  {E}  {R}  {T}";
  text(s, 450, 400, 750, 500); 
  popMatrix();

  pushMatrix();
  textSize(30);
  fill(random(200, 255));
  stroke(0);
  String s1 = "Player 2 choose     {A}  {S}  {D}  {F}  {G}";
  text(s1, 900, 700, 700, 200); 
  popMatrix();
}


void juego() {
  background(0);
  if (j1==p1a) {
    select=0;
    p1a.control();
  }
  if (j1==p2a) {
    select=1;
    p2a.control();
  }
  if (j1==p3a) {
    select=2;
    p3a.control();
  }
  if (j1==p4a) {
    select=3;
    p4a.control();
  }
  if (j1==p5a) {
    select=4;
    p5a.control();
  }
  //Jugador 2
  if (j2==p1b) {
    p2select=0;
    p1a.p2control();
  }
  if (j2==p2b) {
    p2select=1;
    p2a.p2control();
  }
  if (j2==p3b) {
    p2select=2;
    p3a.p2control();
  }
  if (j2==p4b) {
    p2select=3;
    p4a.p2control();
  }
  if (j2==p5b) {
    p2select=4;
    p5a.p2control();
  }


  switch (turno) {
  case 1:
    keyPressed();
    if ((key=='z') || (key == 'Z') ) {
      vida2 -= j1.ataque;
      turno = 2;
    }
    break;
    
  case 2:
    keyPressed();
    if ((key=='m') || (key == 'M') ) {
      vida1 -= j2.ataque;
      turno = 1;
    }
    break;
  }

  pushMatrix();
  fill(random(0, 0), random (0, 255), random(0,255));
  rect(400, 550, (vida1 *20), 40);
  popMatrix();
  if (vida1 <= 0 || vida2<=0) {
    pantalla =3;
  }

  pushMatrix();
  fill(random(0, 0), random (0, 255), random(0,255));
  rect(900, 550, (vida2*20), 40);
  popMatrix();
  if (vida1 <= 0 || vida2<=0) {
    pantalla =3;
  }
  
  textSize(15);
  fill(97, 222, 106);
  stroke(0);
  String s3 = "Player 1 punches with    {Z}";
  text(s3, 410, 410, 600, 200); 

  textSize(15);
  fill(97, 222, 106);
  stroke(0);
  String s4 = "Player 2 punches with    {M}";
  text(s4, 900, 410, 600, 200);
}


void fin() {
  background(0);
  if (vida1 <=0) {
    pushMatrix();
    textSize(50);
    fill(0, random(0, 255), 0);
    stroke(0);
    String s5 = "PLAYER 2 You win!!!";
    text(s5, 600, 410, 600, 200);
    popMatrix();
  }
  if (vida2 <=0) {
    pushMatrix();
    textSize(50);
    fill(random(0, 255), (100),(200));    
    stroke(0);
    String s6 = "PLAYER 1 You win!!!";
    text(s6, 600, 410, 600, 200);
    popMatrix();
  }
  pushMatrix();
  textSize(50);
  fill(random(0, 255), (100),(200));    
  stroke(0);
  String s6 = "Click Mouse to play again!!!";
  text(s6, 600, 610, 600, 200);
  popMatrix();

  if (mousePressed) {
    pantalla = 1;
    player1 = false;
    player2 = false;
    turno = 0;
    vida1 = 0;
    vida2= 0;
  }
}